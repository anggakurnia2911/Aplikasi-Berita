        </activity>
        <activity android:name=".activities.HeadLineActivity">

        </activity>
        <activity android:name=".activities.SportNewsActivity">

        </activity>
        <activity android:name=".activities.OpenNewsActivity">

        </activity>
    </application>

</manifest>